generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Plante {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String
  price       Int
  image       String
  livraison   String
  diametre    Int
  hauteur     Int
}

model User {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}
